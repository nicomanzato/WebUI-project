{
  "name": "redux-boilerplate",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "jest-expo": "^27.0.1",
    "react-native-scripts": "1.14.0",
    "react-test-renderer": "16.3.1",
    "reactotron-react-native": "^2.1.3",
    "reactotron-redux": "^2.1.3",
    "reactotron-redux-saga": "^2.1.3",
    "sinon": "^7.2.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node_modules/.bin/jest"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-navigation-redux-helpers|react-navigation|react-native-safe-area-view|react-navigation-tabs|react-native-tab-view|@expo|expo|sinon)/)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**"
    ]
  },
  "dependencies": {
    "expo": "^27.0.1",
    "mock-socket": "^8.0.5",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-native-elements": "^0.19.1",
    "react-navigation": "^2.3.1",
    "react-navigation-redux-helpers": "^2.0.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  }
}
